{
      "graphs" : [
        {
          "title": "my_metric Value",
          "definition": {
              "events": [],
              "requests": [
                  {"q": "avg:my_metric.value{*}"}
              ],
              "viz": "timeseries"
          }
        },
        {
          "title": "Sum of my_metric Values",
          "description": "Adds up the value of my_metric in 30 second increments",
          "definition": {
              "events": [],
              "requests": [
                  {"q": "sum:my_metric.value.rollup(sum,30)"}
              ],
              "viz": "query_value"
          }
        },
        {
          "title": "MySQL CPU Time",
          "definition": {
              "events": [{"q": "MySQL CPU Anomaly"}],
              "requests": [
                  {"q":"avg:mysql.performance.user_time{*}"}
              ],
              "viz": "timeseries"
          }
        }
      ],
      "title" : "My Vagrant Host Metrics",
      "description" : "A dashboard with my vagrant host metrics",
      "template_variables": [{
          "name": "myhost",
          "prefix": "host",
          "default": "host:ubuntu-xenial"
      }],
      "read_only": "True"
}
