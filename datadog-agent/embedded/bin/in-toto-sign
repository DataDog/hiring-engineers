#!/opt/datadog-agent/embedded/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'in-toto==0.2.3','console_scripts','in-toto-sign'
__requires__ = 'in-toto==0.2.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('in-toto==0.2.3', 'console_scripts', 'in-toto-sign')()
    )
